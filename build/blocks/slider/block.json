{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wp-block-dev/slider",
  "version": "0.1.0",
  "title": "Slider",
  "category": "wp-block-dev-category",
  "description": "Simple slider block.",
  "example": {},
  "attributes": {
    "slidesPerView": {
      "type": "number",
      "default": 3
    },
    "autoplay": {
      "type": "boolean",
      "default": false
    },
    "loop": {
      "type": "boolean",
      "default": false
    },
    "pagination": {
      "type": "boolean",
      "default": false
    },
    "navigation": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "color": {
      "background": true
    },
    "html": false
  },
  "textdomain": "wp-block-dev",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}